@model IEnumerable<Sınav>
<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <a href="/"><i class="fa fa-home"></i>Başvuru</a> / <a href="">Index</a><br />
                <br />
                <br />
                <br />

            </div>
        </div>
        <div class="row" style="display: block;">
            <div class="col-md-12 col-sm-12  ">
                <div class="x_panel">
                    <div class="x_title">
                        <h2><strong><i class="fa fa-bank"></i>Sınav Başvurularınız</strong></h2>
                        <a asp-controller="Basvuru" asp-action="New" class="btn btn-primary float-right" style="background-color:green"><i class="fa fa-plus-circle"></i>Başvuru Oluştur</a>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="table-responsive">
                            <img class="kResim" />
                            <table class="table table-striped jambo_table bulk_action">
                                <thead>
                                    <tr class="headings" id="kullanıcıbilgi">
                                        <th class="column-title">Sınav</th>
                                          <th class="column-title">Unvan</th>
                                        <th class="column-title">Başvuru Tarihleri</th>
                                        <th class="column-title">Başvuru Son Durumu</th>
                                        <th class="column-title"></th>
                                        <th class="column-title"></th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                <tbody>
                                        @foreach (var s in Model)
                                        {

                                        <tr class="even pointer">

                                            <td ><a asp-controller="Basvuru" asp-action="Update" asp-route-id="@s.ID">@s.Name</a></td>
                                         
                                            <td>İnsan Kaynakları Servis Yardımcısı</td>
                                            <td>@s.SınavBaşvuruTarihleri</td>
                                            <td>@s.BasvuruDurum</td>
                                             <td></td>
                                            <td><a href="#" class="btn btn-sm btn-primary" style="background-color:black"> <i class="fa fa-cog"></i>Görüntüle</a></td>
                                               <td><a style="color:#fff;cursor:pointer" class="btn btn-round btn-danger deleteButton" silmeAction="/kullanıcı/sınav/sil/@s.ID">Sil</a></td>
                                        </tr>


                                        }







                                    </tbody>

                                </table>
                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /page content -->
