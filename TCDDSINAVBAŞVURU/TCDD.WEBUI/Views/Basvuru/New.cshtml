                    <!-- Smart Wizard -->
                     <span class="section">Kullanıcı Bilgi</span>

                    <div id="wizard" class="form_wizard wizard_horizontal">
                      
                      <div id="step-1">
                        <form class="form-horizontal form-label-left"  method="post" asp-controller="Basvuru" asp-action="New">

                        
                          <div class="form-group row">
                           <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="@Model.Sınav.Name">
                                </label>
                            <div class="col-md-6 col-sm-6 ">
                        

                         <select class="select2_single form-control" tabindex="-1" asp-for="@Model.Sınav.Name" >
													<option></option>
													<option value="Görevde Yükselme ve Unvan Değişikliği">Görevde Yükselme ve Unvan Değişikliği</option>
													<option value="Unvan Değişikliği">Unvan Değişikliği</option>
													
											
												</select>

										<div class="form-group row">
                         
                            <div class="col-md-6 col-sm-6 ">
                        </div>
                         </div>
                          </div>
                           </div>
                          <div class="item form-group">
                                <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="@Model.Sınav.Unvanlar">
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    @*<select class="form-control" asp-for="@Model.Sınav.Unvanlar" asp-items="@ViewBag.Unvanlar"></select>*@
                                    <select class="form-control"  asp-items="@Model.Unvanlar.Select(s=>new SelectListItem { Value=s.ID.ToString(),Text=s.Name})"></select>
                                </div>
                            </div>
                          <div class="form-group row">
                         <label for="middle-name" class="col-form-label col-md-3 col-sm-3 label-align" asp-for="@Model.Sınav.SınavBaşvuruTarihleri"></label>
                         
                            
                              
                                <div class="col-md-6 col-sm-6 ">
                           <input class="form-control" asp-for="@Model.Sınav.SınavBaşvuruTarihleri" min="1">
                            </div>
                           
                          </div>
                          <div class="form-group row">
                                    <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="@Model.Sınav.BasvuruDurum">
                                </label>
                           
                            <div class="col-md-6 col-sm-6 ">
                        
                                     <select asp-for="@Model.Sınav.BasvuruDurum" asp-items="@Html.GetEnumSelectList<BasvuruDurum>()" style="width:100%;height:38px;margin-bottom:20px; background-color:#fff; color:#212529; border:1px solid #ced4da" >
                       <option value="0">Başvuru Durumu Seçiniz</option>
                         </select>
                         
										<div class="form-group row">
                         
                           <div class="col-md-6 col-sm-6 ">
                        
                        </div>
                        </div>
                          </div>
                          </div>

                          <div class="ln_solid"></div>
                            <div class="item form-group">
                                <div class="col-md-6 col-sm-6 offset-md-3">
                                    <a class="btn btn-primary" asp-controller="Basvuru" asp-action="Index">İptal</a>
                                    <button class="btn btn-primary" type="reset">Temizle</button>
                                    <button type="submit" class="btn btn-success">Gönder</button>
                                </div>
                            </div>
                            @Html.AntiForgeryToken()
                        </form>

                      </div>

                        </div>
                        </div>
                          
                      </div>

                    
